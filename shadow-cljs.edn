{:source-paths ["src"]
 :dependencies [[binaryage/devtools "1.0.3"]
                [metosin/reitit "0.5.18"]
                [nrepl "0.8.3"]
                [reagent "1.1.1"]]
 :builds       {:app {:target     :browser
                      :output-dir "public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [chocologo-cljs.core]}}
                      :devtools   {:after-load chocologo-cljs.core/mount-root}}}

 :dev-http     {3000 {:root    "public"
                      :handler user/app}}}
